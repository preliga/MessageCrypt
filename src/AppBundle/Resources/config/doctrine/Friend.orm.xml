    </many-to-one>
    <many-to-one field="userid2" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="userId2" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
